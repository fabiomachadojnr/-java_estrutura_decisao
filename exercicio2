public class exercicio2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
        Scanner leitor = new Scanner (System.in);
		
		int x , y ;
		
		System.out.println("Digite o primeiro numero:");
		x=leitor.nextInt();
		System.out.println("Digite o segundo numero:");
		y=leitor.nextInt();
			
		if (x+y>10) 
	    System.out.println("Resultado igual a "+(x+y)+" que eh maior que 10.");
		if (x+y==10)
		System.out.println("Resultado igual a "+(x+y)+" que eh igual a 10");
		if (x+y<10)
		System.out.println("Resultado igual a "+(x+y)+" que eh menor que 10");
	}
}