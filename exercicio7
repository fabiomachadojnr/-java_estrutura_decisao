import java.util.Scanner;
public class exercicio7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner leitor = new Scanner (System.in);
		
		String nom1, nom2;
		int id1, id2;
		
		System.out.println("Veja quem eh a pessoa mais velha");
		
		System.out.println("Digite seu nome:");
		nom1 = leitor.nextLine();
		
		System.out.println("Digite sua idade:");
		id1=leitor.nextInt();
		
		System.out.println("Digite seu nome:");
		nom2 = leitor.next();
		
		System.out.println("Digite sua idade:");
		id2=leitor.nextInt();
		
		if (id1>id2)
		System.out.println("A pessoa mais velha eh "+nom1+" com "+id1+" anos.");
		if (id1<id2)
			System.out.println("A pessoa mais velha eh "+nom2+" com "+id2+" anos.");
	}
}